<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>ControladorCliente</servlet-name>
        <servlet-class>Controlador.ControladorCliente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorMascota</servlet-name>
        <servlet-class>Controlador.ControladorMascota</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorReserva</servlet-name>
        <servlet-class>Controlador.ControladorReserva</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ControladorCliente</servlet-name>
        <url-pattern>/ControladorCliente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorMascota</servlet-name>
        <url-pattern>/ControladorMascota</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorReserva</servlet-name>
        <url-pattern>/ControladorReserva</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
